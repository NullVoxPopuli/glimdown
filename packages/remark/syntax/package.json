{
  "name": "@glimdown/remark",
  "version": "0.0.0-alpha.0",
  "description": "Glimdown for remark",
  "author": "NullVoxPopuli",
  "repository": {
    "url": "https://github.com/NullVoxPopuli/glimdown"
  },
  "type": "module",
  "license": "MIT",
  "private": false,
  "files": [
    "src/**/*",
    "README.md"
  ],
  "main": "src/index.js",
  "exports": {
    ".": "./src/index.js"
  },
  "scripts": {
    "build:types": "tsc --build",
    "test": "vitest --coverage --no-watch",
    "test:watch": "vitest --watch",
    "lint": "pnpm -w exec lint",
